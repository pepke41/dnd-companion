{{#paper-toolbar}}
  {{#paper-toolbar-tools}}
    {{#if session.isAuthenticated}}
      {{#link-to "player-handbook" class="navbar"}}Handbook{{/link-to}}
      {{#link-to "characters" class="navbar"}}Characters{{/link-to}}
      <span class="flex"></span>
      {{#paper-button onClick=(route-action "signOut")}}Sign Out{{/paper-button}}

    {{else}}
      <span class="flex"></span>
      {{#paper-button onClick=(route-action "signIn" "google")}}Sign In{{/paper-button}}
    {{/if}}
  {{/paper-toolbar-tools}}
{{/paper-toolbar}}
{{#if session.isAuthenticated}}
  {{outlet}}
{{/if}}
